<head>
  <link rel="icon" type="image/png" href="https://cdn.shopify.com/s/files/1/0155/0195/t/23/assets/fav-icon.png?v=1605894369" />
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
  <title>Synchro by LKSSBV</title>
  <!-- Theme CSS -->
  {{ 'theme.scss.css' | asset_url | stylesheet_tag }}

  <style>
    body {
      background: #c8d4c8;
    }

    #lkssbv {
      position: relative;
    }

    #lkssbv>img {
      position: absolute;
      top: 0px;
      left: 0px;
      right: 0px;
      bottom: 0px;
    }
  </style>

  <script src="https://ajax.googleapis.com/ajax/libs/jquery/1.5.2/jquery.min.js"></script>

  <script type="text/javascript">
    $("#lkssbv > img:gt(0)").hide();

    setInterval(function() {
      $('#lkssbv > img:first')
        .fadeOut(1000)
        .next()
        .fadeIn(1000)
        .end()
        .appendTo('#lkssbv');
    }, 3000);
  </script>

  {{ content_for_header }}
</head>

<body>
  <div id="lkssbv">
    <img src="https://cdn.shopify.com/s/files/1/0155/0195/t/23/assets/lkssbv-1.jpeg?v=1616505535" style="z-index: 4" alt="">
    <img src="https://cdn.shopify.com/s/files/1/0155/0195/t/23/assets/lkssbv-2.jpeg?v=1616505532" style="z-index: 3" alt="">
    <img src="https://cdn.shopify.com/s/files/1/0155/0195/t/23/assets/lkssbv-3.jpeg?v=1616505537" style="z-index: 2" alt="">
    <img src="https://cdn.shopify.com/s/files/1/0155/0195/t/23/assets/lkssbv-4.jpeg?v=1616505540" style="z-index: 1" alt="">
  </div>

  {{ content_for_layout }}
</body>

</html>
