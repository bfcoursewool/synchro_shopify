/*
jQuery.mmenu panels CSS
*/
.mm-menu.mm-horizontal > .mm-panel {
  -webkit-transition: left 0.4s ease;
  -moz-transition: left 0.4s ease;
  -ms-transition: left 0.4s ease;
  -o-transition: left 0.4s ease;
  transition: left 0.4s ease;
}

.mm-menu .mm-hidden {
  display: none;
}

.mm-wrapper {
  overflow-x: hidden;
  position: relative;
}

.mm-menu {
  height: 100%;
  padding: 20px;
  max-width: 350px;
  overflow: scroll;
  background-color: #fff; /**/
}

.mm-menu .empty_cart {
  margin: 20px 0;
}
.mm-menu .slideout-title {
  border-bottom: 1px solid #E2E6E8; /**/
}

.mm-menu .slideout-title .ss-icon {
  font-size: 0 !important;
}

.mm-list .mm-subtitle .cart_title {
  display: inline-block;
  margin: 0 0 20px;
  font-family: 'futura-pt', sans-serif;
  font-size: 16px;
  font-weight: bold; /**/
}

.mm-list .mm-subtitle .ss-icon {
  font-size: 30px;
  color: #e8eced;
  float: right; /**/

  &:after,
  &:before {
    position: absolute;
    content: '';
    height: 3px;
    width: 25px;
  }

  &:before {
    border-top: 3px solid #000;
    transform: rotateZ(45deg);
    transform-origin: -12px -18px;
  }

  &:after {
    border-bottom: 3px solid #000;
    transform: rotateZ(-45deg);
    transform-origin: 13px 34px;
  }
}

.mm-list .mm-subtitle .ss-icon:hover {
  color: #e2554a; /**/
}

.mm-list .cart_item {
  border-bottom: 1px solid #E2E6E8; /**/

}

.mm-list .cart_item .mm-counter {
  position: absolute;
  bottom: 0;
  left: 90px;
  margin: 20px;
  z-index: 1; /**/
}

.mm-list .cart_item .mm-counter .ss-icon {
  width: 25px;
  height: 25px;
  border-radius: 50%;
  font-size: 15px;
  line-height: 22px;
  text-align: center;
  background: #f4f4f5;
  display: inline-block;
  cursor: pointer; /**/
}

.mm-list .cart_item .mm-counter .ss-icon:hover {
  background: #68abc8;
  color: #fff;
}

.mm-list .cart_item .mm-counter input {
  width: 30px;
  display: inline-block;
  margin: 0 5px;
  text-align: center;
  height: inherit;
  padding: 0;
  border: 0; /**/
}

.mm-list .cart_item a  {
  width: 100%;
  height: 120px;
  margin: 20px 0;
  display: block;
  position: relative; /**/
}

.mm-list .cart_item .price{
  position: absolute;
  bottom: 0;
  right: 0;
  font-weight: 500; /**/
}

.mm-list .cart_item .original{
  bottom: 20px;
  font-weight: normal; /**/
  text-decoration: line-through;
  opacity: 0.5; 
}

.mm-list .mm-label .mm-counter {
  position: absolute;
  bottom: 0;
  right: 0;
  margin: 0;
  font-size: 18px;
}

.mm-list .mm-label strong {
  text-transform: none;
  font-weight: normal; /**/
}
.mm-list .mm-subtitle.clearfix {
  background: #f4f4f5;
  margin: 20px -20px -20px;
  padding: 20px;
}

.mm-list .mm-subtitle.clearfix input {
  width: 100%;
}

.mm-list .mm-subtitle.clearfix .edit_cart {
  font-family: 'futura-pt', sans-serif;
  text-decoration: underline;
  text-align: center;
  margin-top: 15px;
  display: block;
  color: #2F90D9; /**/
}

.mm-menu input[type=number]::-webkit-inner-spin-button,
.mm-menu input[type=number]::-webkit-outer-spin-button {
  -webkit-appearance: none;
  margin: 0; /**/
}
